{
  "name": "nazca-youniverse",
  "version": "1.0.0",
  "private": true,
  "main": "main/main.mjs",
  "type": "module",
  "scripts": {
    "dev": "node server.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron-dev": "concurrently \"npm:dev\" \"wait-on http://localhost:3000 && npm:electron\"",
    "build-electron": "dotenv -e .env -- electron-builder --config electron-builder.json --publish never",
    "build-electron-full": "npm run build && dotenv -e .env -- electron-builder --config electron-builder.json --publish never",
    "dist": "npm run build && electron-builder --publish=never",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@vercel/analytics": "^1.5.0",
    "@vis.gl/react-google-maps": "^1.5.3",
    "concurrently": "^9.1.2",
    "electron-serve": "^2.1.1",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "extract-zip": "^2.0.1",
    "next": ">=15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "tailwindcss-animated": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "next": ">=15.3.4",
    "postcss": "^8",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "wait-on": "^8.0.5"
  }
}
